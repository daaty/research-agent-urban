{
    "name": "ai-agent-tool",
    "version": "1.0.0",
    "description": "AI Agent creation tool - Pilot project",
    "main": "dist/app-monitoring.js",
    "scripts": {
        "build": "tsc --skipLibCheck",
        "start": "node dist/app-monitoring.js",
        "start:simple": "node dist/app-simple.js",
        "dev": "ts-node-dev --respawn --transpile-only src/app-simple.ts",
        "dev:monitor": "ts-node-dev --respawn --transpile-only src/app-monitoring.ts",
        "scrape-once": "ts-node src/scraper/ridesScrapingSimple.ts",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prod": "npm run build && npm start",
        "install:deps": "npm install --production",
        "setup": "npm install && npm run build",
        "puppeteer-install-chrome": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=false npm install puppeteer",
        "playwright-install": "npx playwright install chromium",
        "install-browsers": "npm run playwright-install && npm run puppeteer-install-chrome"
    },
    "dependencies": {
        "axios": "^1.10.0",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "node-cron": "^4.1.0",
        "node-fetch": "^2.7.0",
        "playwright": "^1.52.0",
        "puppeteer": "^19.11.1"
    },
    "devDependencies": {
        "@types/express": "^5.0.1",
        "@types/node": "^22.15.2",
        "@types/node-cron": "^3.0.11",
        "@types/node-fetch": "^2.6.12",
        "rimraf": "^6.0.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "author": "",
    "license": "MIT"
}
